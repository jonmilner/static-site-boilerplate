{
  "use": [
    "postcss-import",
    "postcss-apply",
    "postcss-mixins",
    "postcss-nested",
    "cq-prolyfill/postcss-plugin",
    "precss",
    "postcss-cssnext",
    "postcss-assets",
    "postcss-inline-svg",
    "postcss-font-magician",
    "postcss-initial",
    "postcss-pxtorem",
    "postcss-reporter"
  ],
  "input": "assets/css/*.css",
  "dir": "dist/assets/css",
  "map": false,
  "local-plugins": true,
  "autoprefixer": {
    "browsers": "> 2%"
  },
  "postcss-assets": {
    "basePath": "src/",
    "cachebuster": true,
    "loadPaths": ["img/"],
    "relative": true
  },
  "postcss-font-magician": {
    "foundries": ["custom", "hosted", "google"]
  },
  "postcss-pxtorem": {
    "unitPrecision": 10,
    "propWhiteList": [],
    "replace": false,
    "selectorBlackList": [],
    "minPixelValue": 5
  },
  "cssnano": {
    "autoprefixer": false,
    "calc": false,
    "colormin": false,
    "core": true,
    "discardComments": {
      "removeAll": true
    },
    "discardUnused": false,
    "filterPlugins": false,
    "normalizeUrl": false,
    "reduceIdents": false,
    "styleCache": true,
    "zindex": false
  },
  "postcss-reporter": {
    "clearMessages": true
  }
}
